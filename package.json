{
    "name": "vscode-gedcom",
    "displayName": "GEDCOM Language",
    "description": "GEDCOM file language support for VS Code",
    "version": "0.0.3",
    "preview": true,
    "publisher": "florianguitton",
    "license": "Apache-2.0",
    "author": {
        "name": "Florian Guitton",
        "url": "https://github.com/fguitton"
    },
    "keywords": [
        "gedcom",
        "ged",
        "genealogy"
    ],
    "homepage": "https://github.com/fguitton/vscode-gedcom/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/fguitton/vscode-gedcom"
    },
    "bugs": {
        "url": "https://github.com/fguitton/vscode-gedcom/issues"
    },
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#3a3d41",
        "theme": "dark"
    },
    "contributes": {
        "languages": [
            {
                "id": "gedcom",
                "aliases": [
                    "GEDCOM",
                    "gedcom"
                ],
                "extensions": [
                    ".ged",
                    ".gedcom"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "gedcom",
                "scopeName": "source.gedcom",
                "path": "./syntaxes/gedcom.tmLanguage.json"
            }
        ]
    }
}